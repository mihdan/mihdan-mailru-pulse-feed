{
  "name": "mihdan/mihdan-mailru-pulse-feed",
  "description": "Плагин, формирует ленту для рекомендательной системы Пульс от компании Mail.ru",
  "license": "GPL-2.0",
  "keywords": [
    "cyrillic, georgian, latin, l10n, russian, rustolat, slugs, translations, transliteration"
  ],
  "homepage": "https://github.com/mihdan/mihdan-mailru-pulse-feed",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "Mikhail Kobzarev",
      "email": "mikhail@kobzarev.com",
      "homepage": "https://www.kobzarev.com/"
    }
  ],
  "support": {
    "issues": "https://github.com/mihdan/mihdan-mailru-pulse-feed/issues"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type":"composer",
      "url":"https://wpackagist.org"
    }
  ],
  "require": {
    "composer/installers": "*",
    "wptrt/admin-notices": "^1.0",
    "ext-dom": "*",
    "ext-libxml": "*",
    "imangazaliev/didom": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^2.9",
    "phpcompatibility/php-compatibility": "^9.0",
    "wp-coding-standards/wpcs": "^1.2",
    "wp-cli/wp-cli": "*"
  },
  "autoload": {
    "classmap": [
      "includes"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "includes"
    ]
  }
}
